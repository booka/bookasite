<% title @series.title %>

<%= render_top do %>
    <%= render :partial => 'series/header', :locals => {:series => @series} %>
<% end %>

<div class="row24">
  <div id="browser" class="col-left">
    <%= link_to image_tag(@series.icon_path, :alt => @series.title), root_path %>
    <%= render_browser :projects, @series.projects %>

  </div>
  <div class="col-center">
    <%= render_body @series %>
    <%= disqus @series %>
  </div>
  <div id="info" class="col-right">
    <%= render_body @series.page %>
  </div>
</div>



