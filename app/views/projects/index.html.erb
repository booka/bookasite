<% title 'Convocatorias'%>
<div class="browser">
  <h3>Convocatorias</h3>
  <ul class="calls">
    <% @calls.each do |call| %>
      <li class="call">
        <%= link_to call.title, project_call_path(call.project) %>
        <div class="property description"><%= call.description %></div>
      </li>
    <% end %>
  </ul>
</div>
<div class="center">
  <div class="page">
    <h1><%= @page.title %></h1>
    <%= @page.body.html_safe %>
  </div>
</div>
<div class="places">
</div>