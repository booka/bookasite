<% title "Participantes" %>

<%= render_top :users do %>
    <% unless project? %>
        <h1>Participantes</h1>
    <% end %>
<% end %>

<div class="row24">
  <div id="browser" class="col-left">
    &nbsp;
  </div>
  <div class="col-center">
    <h1>Participantes</h1>
      <% users.each do |user| %>
        <%= render user %>
      <% end %>
  </div>
  <div id="info" class="col-right">
  </div>
</div>


