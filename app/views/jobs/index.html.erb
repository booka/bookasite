<% title "Trabajos" %>

<%= render_top :users do %>
    <h1>Trabajos</h1>
<% end %>

<div class="row24">
  <div id="browser" class="col-left">
    &nbsp;
  </div>
  <div class="col-center">
    <table>
      <% jobs.each do |job| %>
          <tr class="<%= cycle('even', 'odd') %>">
            <td><%= job.priority %></td>
            <td><%= job.name %></td>
            <td><%= job.user.name %></td>
            <td><%= time_ago_in_words job.created_at %></td>
          </tr>
      <% end %>
    </table>
  </div>
  <div id="info" class="col-right">
  </div>
</div>


