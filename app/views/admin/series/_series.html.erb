<div class="actions">
  <%#= link_to 'Crear nueva serie', [:admin, :series] %>
</div>

<table>
  <thead>
    <tr>
      <td>Nombre</td>
      <td></td>
    </tr>
  </thead>
  <tbody>
    <% @series.each do |serie| %>
      <tr class="<%= cycle('even', 'odd') %>">
        <td class="title"><%= link_to serie.name, [:admin, serie] %></td>
        <td class="actions">
          <%= link_to 'Administrar', admin_series_path(serie) %>
          <%#= link_to 'Borrar', admin_project_path(project), :method => :delete,
          :confirm => "Quieres borrar el proyecto '#{project.title}'!! No se puede volver atrás!!. ¿Segura?" %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
